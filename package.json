{
  "name": "vue-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:integration": "cypress run",
    "test:integration:gui": "cypress run --headed --no-exit --browser chrome",
    "lint": "vue-cli-service lint",
    "server": "json-server --watch db.json",
    "start": "npm run serve"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.28.1",
    "@vuepic/vue-datepicker": "^3.3.1",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "cypress": "^10.4.0",
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "pinia": "^2.0.14",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@pinia/testing": "^0.0.13",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "json-server": "^0.17.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^13.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
